{
  "name": "cherico-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && rm -rf public",
    "start": "run-s webpack:dev",
    "build": "run-s clean webpack",
    "webpack": "env-cmd -f .env webpack -p",
    "webpack:dev": "env-cmd -f .env webpack-dev-server -d",
    "deploy:ci": "webpack -p --env.ci",
    "lint": "run-s 'eslint -- --fix' 'stylelint -- --fix'",
    "lint:ci": "eslint",
    "eslint": "eslint --cache --config .eslintrc.js ./src",
    "stylelint": "stylelint --cache --config stylelint.config.js ./src/**/*.{css,scss,sass}",
    "deploy": "run-s build deploy:firebase",
    "deploy:firebase": "firebase deploy --only hosting",
    "test": "jest --all --colors --silent=true"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "axios": "^0.20.0",
    "d3": "^6.6.2",
    "date-fns": "^2.16.1",
    "env-cmd": "^10.1.0",
    "firebase": "^7.21.0",
    "firebase-tools": "^8.10.0",
    "joi-browser": "^13.4.0",
    "moment": "^2.28.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-id-swiper": "^4.0.0",
    "react-linkify": "^1.0.0-alpha",
    "react-nl2br": "^0.6.0",
    "react-router-dom": "^5.2.0",
    "smoothscroll-polyfill": "^0.4.4",
    "swiper": "^6.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "autoprefixer": "^9.8.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.1.1",
    "core-js": "^3.6.5",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^5.1.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": ">=4",
    "jest": "^26.4.2",
